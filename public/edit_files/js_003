}};

/* js/apps/ProfileLinkChanger.js */

LI.define("Profile");
LI.Profile.LinkChanger=function(c,b){b=b||{};
b={baseUrl:b.baseUrl||"",selector:b.selector||"a.option",useDialog:b.useDialog||true,overrideUrlParams:b.overrideUrlParams||true};
if(b.overrideUrlParams){var a=b.baseUrl.indexOf("?");
if(a!=-1){b.baseUrl=b.baseUrl.substring(0,a)
}}var d=Y$(b.selector,c);
YEvent.on(d,"click",function(f){var j=YEvent.getTarget(f),h;
if(!j.href){h=YDom.getAncestorBy(j,function(k){if(k.href){return true
}else{return false
}})
}else{h=j
}var i=h.href;
var g=b.baseUrl;
if(b.overrideUrlParams){var e=i.indexOf("?");
g=g+(e==-1?"":i.substr(e))
}if(b.useDialog){LI.Dialog().swap({content:{url:g}});
YEvent.preventDefault(f)
}})
};

/* js/badge_web_tracking.js */

var badgeWebTracking=function(){var a="premiumicon",e="jobseekericon",b="openlinkicon";
var d=function(i){YEvent.preventDefault(i);
var f=YEvent.getTarget(i);
if(f&&f.href){var g=f.href,h=LI.getPageKey();
if(YDom.hasClass(f,a)){WebTracking.trackInBadgeClicked(h,g)
}else{if(YDom.hasClass(f,e)){WebTracking.trackJssBadgeClicked(h,g)
}else{if(YDom.hasClass(f,b)){WebTracking.trackOlBadgeClicked(h,g)
}}}}};
var c=function(){if(!WebTracking){return false
}var f=Y$("#body .wrapper #main a.accounticon");
YEvent.on(f,"click",d)
};
return{init:c}
}();
YEvent.onDOMReady(badgeWebTracking.init);